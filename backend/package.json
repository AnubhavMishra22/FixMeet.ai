{
  "name": "fixmeet-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && npm run copy-migrations",
    "copy-migrations": "node -e \"const fs=require('fs');const p=require('path');const s=p.join('src','db','migrations');const d=p.join('dist','db','migrations');fs.mkdirSync(d,{recursive:true});fs.readdirSync(s).filter(f=>f.endsWith('.sql')).forEach(f=>fs.copyFileSync(p.join(s,f),p.join(d,f)));console.log('Migrations copied to dist/')\"",
    "postinstall": "npm run build",
    "start": "node dist/server.js",
    "db:migrate": "tsx src/db/migrate.ts"
  },
  "dependencies": {
    "@langchain/core": "^1.1.24",
    "@langchain/google-genai": "^2.1.18",
    "@langchain/langgraph": "^1.1.4",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "googleapis": "^171.4.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "postgres": "^3.4.3",
    "resend": "^6.9.1",
    "typescript": "^5.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0"
  },
  "engines": {
    "node": ">=20"
  }
}
